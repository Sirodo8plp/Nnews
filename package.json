{
  "name": "news_site",
  "version": "1.0.0",
  "description": "News Site to practice new concepts",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss style.comp.css",
    "concat:css": "concat -o style.concat.css style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css style.prefix.css",
    "build:css": "npm-run-all compile:sass concat:css prefix:css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "newsapi": "^2.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.1",
    "body-parse": "^0.1.0",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "mongoose": "^5.11.18",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^5.0.0",
    "request": "^2.88.2",
    "unirest": "^0.6.0"
  }
}
